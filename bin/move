#!/usr/bin/env ruby

require_relative '../lib/move.rb'

# Code your CLI Here
puts "Welcome to Tic Tac Toe!"
display_board(board)
puts "Where would you like to go?"
input = gets.strip.to_i               #strip out whitespaces on the string and convert to int
