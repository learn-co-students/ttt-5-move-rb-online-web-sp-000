#!/usr/bin/env ruby

require_relative '../lib/move.rb'

# Code your CLI Here

puts "Welcome to Tic Tac Toe!"
puts "Where would you like to go?"

board = [" ", " ", " ", " ", " ", " ", " ", " ", " "]

display_board(board)
number = gets.to_i
index_number = input_to_index(number)
move(board, index_number)
display_board(board)
