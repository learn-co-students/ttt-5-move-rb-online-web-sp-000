#!/usr/bin/env ruby

require_relative '../lib/move.rb'

# Code your CLI Here

puts "Welcome to Tic Tac Toe!"

board = [" ", " ", " ", " ", " ", " ", " ", " ", " "]
display_board(board)

puts "Where yould you like to go?"


input = gets.strip
input_to_index(input)

index = input.to_i-1
move(board, index, character = "X")



display_board(board)











