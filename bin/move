#!/usr/bin/env ruby

require_relative '../lib/move.rb'

# Code your CLI Here
puts "Welcome to Tic Tac Toe!"
puts "Where would you like to go?"
puts "Choose a number between 1-9"
input = gets.strip
value = "X"
board = [" ", " ", " ", " ", " ", " ", " ", " ", " "]
move(board,input_to_index(input), value)
display_board(board)